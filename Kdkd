if getgenv().Fullbright then
    local Lighting = game.Lighting
    Lighting.ClockTime = 12
    Lighting.Brightness = 2
    Lighting.FogEnd = 100000
    Lighting.GlobalShadows = false
    Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
    wait(0.1)
    local Lightingz = game:GetService("Lighting") -- Corrected assignment here
    for i, v in pairs(Lightingz:GetChildren()) do
        if v.ClassName == "Atmosphere" then
            v:Destroy() -- Added a colon for method call
        end
    end
end

loadstring(game:HttpGet("https://raw.githubusercontent.com/MerryXTrash/Main.lua/refs/heads/main/Checkgame.lua40"))()
