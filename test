id == 7265397848 or id == 7251867574
local TweenService = game:GetService("TweenService")
local Workspace = game:GetService("Workspace")
local BossBattle = Workspace:FindFirstChild("BossBattle")
local Saigomo = BossBattle:FindFirstChild("Saigomo")

if Saigomo then
    -- Target CFrame, moved up by 10 units on the Y axis
    local targetCFrame = Saigomo.CFrame * CFrame.new(0, 10, 0)
    
    -- TweenInfo: duration of 1 second, Linear easing style, InOut easing direction
    local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
    
    -- Goal: the target CFrame
    local goal = {CFrame = targetCFrame}
    
    -- Create the tween
    local tween = TweenService:Create(Saigomo, tweenInfo, goal)
    
    -- Play the tween
    tween:Play()
end
